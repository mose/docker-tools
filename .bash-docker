#!/bin/bash

container_ip() {
  docker inspect -f '{{.NetworkSettings.IPAddress}}' $1
}

docker_tools() {
  echo "Usage:"
  echo "  d_ip <container> : spits the IP of a given container"
  echo "                     <container> can be a hash or a name"
}

alias d_ip=container_ip
alias docker-tools=docker_tools
